{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "vidiun-ngx-client": {
      "root": "projects/vidiun-ngx-client",
      "sourceRoot": "projects/vidiun-ngx-client/src",
      "projectType": "library",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-ng-packagr:build",
          "options": {
            "tsConfig": "projects/vidiun-ngx-client/tsconfig.lib.json",
            "project": "projects/vidiun-ngx-client/ng-package.json"
          },
          "configurations": {
            "production": {
              "project": "projects/vidiun-ngx-client/ng-package.prod.json"
            }
          }
        }
      }
    }
  },
  "defaultProject": "vidiun-ngx-client"
}
